{
  "name": "@ghntech/n8n-nodes-gtalk",
  "version": "0.1.1",
  "description": "This is a community node for n8n to interact with the Ghntech Gtalk API. It allows you to automate tasks related to Gtalk, such as sending messages.",
  "license": "MIT",
  "homepage": "https://gtalk.ghn.vn",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Phát Làu",
    "email": "phatlv@ghn.vn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghntech/n8n-nodes-gtalk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GhntechGtalkApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/GhntechGtalk/GhntechGtalk.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
